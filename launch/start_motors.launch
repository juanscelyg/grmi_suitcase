<?xml version="1.0" encoding="utf-8"?>

<launch>
  <!-- Parameters -->
  <param name="power_gain"           value="0.5" />
  <param name="speed_gain"           value="0.75" />
  <param name="omega_gain"           value="0.75" />

  <!-- Start Motors -->
  <!--node pkg="grmi_suitcase" type="motors_control.py" name="motors_control" output="screen">
    <param name="power_gain"           value="$(arg power_gain)" />
    <param name="speed_gain"           value="$(arg speed_gain)" />
    <param name="omega_gain"           value="$(arg omega_gain)" />
  </node-->
  <!-- Launch the node to get the values of the mechanism -->
<node pkg="rosserial_python" type="serial_node.py" name="serial_node">
  <param name="port" value="/dev/ttyACM0"/>
  <param name="baud" value="57600"/>
</node>

</launch>
